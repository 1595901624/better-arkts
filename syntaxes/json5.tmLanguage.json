{
  "scopeName": "source.json5",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#keys"
    },
    {
      "include": "#operators"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.json5",
          "match": "//.*$",
          "captures": {
            "0": {
              "name": "punctuation.definition.comment.json5"
            }
          }
        },
        {
          "name": "comment.block.json5",
          "begin": "/\\*",
          "end": "\\*/",
          "captures": {
            "0": {
              "name": "punctuation.definition.comment.json5"
            }
          }
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.language.json5",
          "match": "\\b(true|false|null)\\b"
        },
        {
          "name": "constant.language.json5",
          "match": "\\b(Infinity|-Infinity|NaN)\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.json5",
          "match": "\\b(0[xX][0-9a-fA-F]+|0[oO][0-7]+|0[bB][01]+)\\b"
        },
        {
          "name": "constant.numeric.json5",
          "match": "\\b(\\d+\\.\\d*|\\.\\d+)([eE][+-]?\\d+)?\\b"
        },
        {
          "name": "constant.numeric.json5",
          "match": "\\b\\d+([eE][+-]?\\d+)?\\b"
        },
        {
          "name": "constant.numeric.json5",
          "match": "\\b[+-]?Infinity\\b"
        },
        {
          "name": "constant.numeric.json5",
          "match": "\\bNaN\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.single.json5",
          "begin": "'",
          "end": "'",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.json5"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.json5"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.json5",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.double.json5",
          "begin": "\"",
          "end": "\"",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.json5"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.json5"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.json5",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.multi.json5",
          "begin": "'''",
          "end": "'''",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.json5"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.json5"
            }
          }
        }
      ]
    },
    "keys": {
      "patterns": [
        {
          "name": "support.type.property-name.json5",
          "match": "(?<=\\{\\s*|,\\s*)\\s*([a-zA-Z_$][a-zA-Z0-9_$]*)\\s*(?=:)",
          "captures": {
            "1": {
              "name": "entity.name.tag.json5"
            }
          }
        },
        {
          "name": "string.quoted.double.json5",
          "match": "(?<=\\{|,)\\s*(\"[^\"]*\")\\s*(?=:)",
          "captures": {
            "1": {
              "name": "entity.name.tag.json5"
            }
          }
        },
        {
          "name": "string.quoted.single.json5",
          "match": "(?<=\\{|,)\\s*('[^']*')\\s*(?=:)",
          "captures": {
            "1": {
              "name": "entity.name.tag.json5"
            }
          }
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "punctuation.separator.object.json5",
          "match": ":"
        },
        {
          "name": "punctuation.separator.array.json5",
          "match": ","
        },
        {
          "name": "punctuation.definition.block.json5",
          "match": "[{}]"
        },
        {
          "name": "punctuation.definition.array.begin.json5",
          "match": "\\["
        },
        {
          "name": "punctuation.definition.array.end.json5",
          "match": "\\]"
        }
      ]
    }
  }
}
